{
  "name": "hound",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Hound Phase 1 desktop app",
  "main": "electron/main.cjs",
  "scripts": {
    "dev:web": "vite --config ui/vite.config.js",
    "dev:app": "concurrently \"npm run dev:web\" \"npm run electron\"",
    "start": "npm run dev:app",
    "build": "vite build --config ui/vite.config.js",
    "preview": "vite preview --config ui/vite.config.js",
    "electron": "electron ./electron/main.cjs"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.2",
    "electron": "^27.1.2",
    "vite": "^5.0.3"
  }
}
